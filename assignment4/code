            let ad = {
                item: 'Cars for sale',
                where: 'in your area!',
            };
            document.getElementById("ad").innerHTML = ad.item + " " + ad.where;

            let person1 = {
                name: 'Ricardo',
                city: 'Corvallis',
                age: 26,
                isCool: true,
                updateAge : function (){
                    return ++person1.age;
                }
            
            };
            // document.getElementById("profile").innerHTML = person1;
            document.getElementById("profile1").innerHTML = person1.name + ", " + person1.city + ", " + person1.age;
            

            if(person1.isCool == true) {
                console.log(person1.name + "is cool!!");
            }
            else { (person1.isCool == false)
                console.log(person1.name + "is not cool!") 
            }


            if(person1.city == 'Corvallis') {
                console.log("You are from Corvallis!") 
            } 
            else {
                console.log("You are not from Corvallis! Boo!") 
            }
            let person = new Object();
    
            person.name = "Ricardo";
            person.city = "Corvallis";
            person.age = 26;
            person.updateAge = function (){
                return ++person.age;
            }
            
            console.log(person.age);
            person.updateAge();
            console.log(person.age);
    
            let selectedColors = ['red', 'blue','green','yellow','black','white'];
            selectedColors.forEach(element => {
                document.getElementById("carcolors").innerHTML = element;
                console.log(element);
            })
